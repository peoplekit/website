# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# 2 space indentation
indent_style = space
indent_size = 2

# max line length
max_line_length = 200

# Set default charset
charset = utf-8

trim_trailing_whitespace=true